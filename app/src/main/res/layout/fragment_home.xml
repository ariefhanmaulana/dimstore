<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    tools:context=".HomeFragment">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <EditText
            android:id="@+id/editTextText"
            android:layout_width="333dp"
            android:layout_height="49dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="24dp"
            android:layout_marginTop="16dp"
            android:layout_marginBottom="25dp"
            android:ems="10"
            android:inputType="text"
            android:text="Search" />

        <ImageView
            android:id="@+id/imageView2"
            android:layout_width="27dp"
            android:layout_height="27dp"
            android:layout_alignParentEnd="true"
            android:layout_marginTop="28dp"
            android:layout_marginEnd="14dp"
            android:background="@drawable/cartmerah2">

        </ImageView>

        <ImageButton
            android:id="@+id/imageView"
            android:layout_width="368dp"
            android:layout_height="339dp"
            android:layout_below="@+id/imageView2"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="21dp"
            android:background="@drawable/ganteng">

        </ImageButton>

        <ImageView
            android:id="@+id/imageView10"
            android:layout_width="185dp"
            android:layout_height="72dp"
            android:layout_below="@+id/imageView"
            android:layout_marginTop="17dp"
            android:layout_marginEnd="9dp"
            android:layout_toStartOf="@+id/imageView13"
            android:background="@drawable/pink">

        </ImageView>

        <ImageView
            android:id="@+id/imageView12"
            android:layout_width="188dp"
            android:layout_height="63dp"
            android:layout_below="@+id/imageView10"
            android:layout_marginTop="15dp"
            android:layout_marginEnd="7dp"
            android:layout_toStartOf="@+id/imageView13"
            android:background="@drawable/cokllat">

        </ImageView>

        <ImageView
            android:id="@+id/imageView13"
            android:layout_width="175dp"
            android:layout_height="148dp"
            android:layout_below="@+id/imageView"
            android:layout_alignParentEnd="true"
            android:layout_marginTop="18dp"
            android:layout_marginEnd="22dp"
            android:background="@drawable/rak2">

        </ImageView>

        <ImageView
            android:id="@+id/imageView15"
            android:layout_width="179dp"
            android:layout_height="152dp"
            android:layout_below="@+id/imageView13"
            android:layout_alignParentEnd="true"
            android:layout_marginStart="24dp"
            android:layout_marginTop="7dp"
            android:layout_marginEnd="20dp"
            android:background="@drawable/rak3">

        </ImageView>

        <ImageView
            android:id="@+id/imageView14"
            android:layout_width="186dp"
            android:layout_height="140dp"
            android:layout_below="@+id/imageView12"
            android:layout_alignParentEnd="true"
            android:layout_marginTop="7dp"
            android:layout_marginEnd="204dp"
            android:background="@drawable/rak">

        </ImageView>
    </RelativeLayout>





</FrameLayout>